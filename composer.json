{
    "name": "saritasa/laravel-chat-api",
    "type": "library",
    "description": "Classes, that implement chat API on top of laravel",
    "keywords": ["laravel", "api", "chat"],
    "license": "MIT",
    "authors": [
        {
            "name": "Sergey Populov",
            "email": "sergey@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "illuminate/http": "^5.5 || ^6.0",
        "illuminate/support": "^5.5 || ^6.0",
        "saritasa/laravel-entity-services": "^1.0",
        "dingo/api": "^2.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "phpunit/phpunit": "^6.5",
        "squizlabs/php_codesniffer": "^3.5"
    },
    "minimum-stability": "stable",
    "autoload": {
        "psr-4" : {
            "Saritasa\\LaravelChatApi\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Saritasa\\LaravelChatApi\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0.x-dev"
        },
        "laravel": {
            "providers": [
                "Saritasa\\LaravelChatApi\\LaravelChatApiServiceProvider"
            ]
        }
    }
}
